{
  "expo": {
    "name": "nearsy-app",
    "slug": "nearsy-app",
    "scheme": "nearsy",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nearsy.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "...",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "...",
        "NSLocationAlwaysUsageDescription": "...",
        "NSPhotoLibraryUsageDescription": "...",
        "NSPhotoLibraryAddUsageDescription": "...",
        "NSCameraUsageDescription": "...",
        "UIBackgroundModes": ["location", "location"]
      },
      "usesAppleSignIn": true
    },
    "android": {
      "package": "com.nearsy.app",
      "versionCode": 2,
      "edgeToEdgeEnabled": true,
      "windowSoftInputMode": "adjustResize",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION",
        "android.permission.READ_MEDIA_IMAGES"
      ]
    },
    "notification": {
      "icon": "./src/assets/notification-icon.png",
      "color": "#3B5A85",
      "channelId": "default"
    },
    "web": {
      "favicon": "./src/assets/icon.png"
    },
    "runtimeVersion": "1.0.1",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/fdb12a0f-6136-465d-bbf7-1eda6dcc14de"
    },
    "plugins": [
      "expo-dev-client",
      "expo-image-picker",
      [
        "expo-location",
        {
          "isAndroidForegroundServiceEnabled": true
        }
      ],
      ["expo-notifications"],
      "expo-asset",
      "expo-font",
      "expo-web-browser",
      "react-native-edge-to-edge"
    ],
    "extra": {
      "eas": {
        "projectId": "fdb12a0f-6136-465d-bbf7-1eda6dcc14de"
      },
      "EXPO_PUBLIC_ENABLE_SOCIAL_LOGIN": "false",
      "EXPO_PUBLIC_GOOGLE_EXPO_CLIENT_ID": "",
      "EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID": "557470198780-prau0p14tfrijv3h5omadqakn4r3m4tk.apps.googleusercontent.com",
      "EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID": "557470198780-9qe3qg59h682e3fn14ic97hf9t375m3n.apps.googleusercontent.com",
      "EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID": "557470198780-8ls8o304sfcf8va409va90o6rar3vc5a.apps.googleusercontent.com",
      "EXPO_PUBLIC_APPLE_SERVICE_ID": "com.nearsy.app.signin",
      "EXPO_PUBLIC_APPLE_REDIRECT_WEB": "https://auth.expo.io/@godie.hurtado/nearsy-app"
    },
    "owner": "godie.hurtado"
  }
}
